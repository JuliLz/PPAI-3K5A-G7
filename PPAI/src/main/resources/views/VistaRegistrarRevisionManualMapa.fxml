<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.grupo7.dsi.tpai.boundaries.PantallaRegistrarRevisionManual"
            prefWidth="800" prefHeight="600"
            style="-fx-background-color: #f4f4f4;">

    <VBox spacing="20" AnchorPane.topAnchor="20" AnchorPane.bottomAnchor="20"
          AnchorPane.leftAnchor="20" AnchorPane.rightAnchor="20"
          style="-fx-alignment: center;">

        <ImageView fitWidth="760" fitHeight="540" preserveRatio="true">
            <image>
                <Image url="/views/mapaSismo.png"/>
            </image>
        </ImageView>

        <Button text="Volver" onAction="#continuarCerrarDetalleEvento"
                style="-fx-font-size: 14px; -fx-padding: 5 20 5 20;" />
    </VBox>
</AnchorPane>